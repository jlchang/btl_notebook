    "20170512\n",
    "use .r-3.3.2\n",
    "R\n",
    "library(ChIPQC)\n",
    "load(\"/broad/hptmp/jlchang/dogChIPQCtest1/humantest2Ab/ChIPQC.RData\")\n",
    "#loads report object, not data object, oh well\n",
    "\n",
    "humantestK4me3 <- ChIPQC(\"/broad/hptmp/jlchang/dogChIPQCtest1/humantestK4me3.csv\",,\"1\")\n",
    "save(humantestK4me3, file= \"/broad/hptmp/jlchang/dogChIPQCtest1/humantestK4me3/humantestK4me3.RData\")\n",
    "\n",
    "humantestK4me3_all <- ChIPQC(\"/broad/hptmp/jlchang/dogChIPQCtest1/humantestK4me3.csv\")\n",
    "save(humantestK4me3_all, file= \"/broad/hptmp/jlchang/dogChIPQCtest1/humantestK4me3/humantestK4me3_all.RData\")\n",
    "\n",
    "ChIPQCreport(experiment = \"foo\", facet=TRUE,reportName=\"dogChIPQC\",reportFolder=\"ChIPQC\",facetBy=\"Factor\",colourBy=c(\"Replicate\"))\n",
    "\n",
    "\n",
    "\n",
    "Had to delete empty bed files and bed files that only had headers and bed that had low number of peaks\n",
    "\n",
    "ChIPQCreport(humantestK4me3,reportFolder=\"/broad/hptmp/jlchang/dogChIPQCtest1/humantestK4me3\", facet=FALSE)"
